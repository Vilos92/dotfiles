	"subtitle": "Currently Selected",
	"match": "",
	"valid": "false"
'

cat &lt;&lt; EOB
{"items": [
	{
		"title": "$default",
		"arg": "",
		$([[ -z ${(P)setting} ]] &amp;&amp; echo $currSelected)
	},
	{
		"title": "Arc",
		"arg": "company.thebrowser.Browser",
		$([[ ${(P)setting} == "company.thebrowser.Browser" ]] &amp;&amp; echo $currSelected)
	},
	{
		"title": "Chromium",
		"arg": "org.chromium.Chromium",
		$([[ ${(P)setting} == "org.chromium.Chromium" ]] &amp;&amp; echo $currSelected)
	},
	{
		"title": "Firefox",
		"arg": "org.mozilla.firefox",
		$([[ ${(P)setting} == "org.mozilla.firefox" ]] &amp;&amp; echo $currSelected)
	},
	{
		"title": "Google Chrome",
		"arg": "com.google.Chrome",
		$([[ ${(P)setting} == "com.google.Chrome" ]] &amp;&amp; echo $currSelected)
	},
	{
		"title": "Microsoft Edge",
		"arg": "com.microsoft.edgemac",
		$([[ ${(P)setting} == "com.microsoft.edgemac" ]] &amp;&amp; echo $currSelected)
	},
	{
		"title": "Opera",
		"arg": "com.operasoftware.Opera",
		$([[ ${(P)setting} == "com.operasoftware.Opera" ]] &amp;&amp; echo $currSelected)
	},
	{
		"title": "Orion",
		"arg": "com.kagi.kagimacOS",
		$([[ ${(P)setting} == "com.kagi.kagimacOS" ]] &amp;&amp; echo $currSelected)
	},
	{
		"title": "Safari",
		"arg": "com.apple.Safari",
		$([[ ${(P)setting} == "com.apple.Safari" ]] &amp;&amp; echo $currSelected)
	},
	{
		"title": "Zen",
		"arg": "app.zen-browser.zen",
		$([[ ${(P)setting} == "app.zen-browser.zen" ]] &amp;&amp; echo $currSelected)
	}
]}
EOB</string>
